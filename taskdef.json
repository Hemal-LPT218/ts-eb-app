{
  "family": "AppTask",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::730335183789:role/ECSTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "730335183789.dkr.ecr.us-east-1.amazonaws.com/demo-repository/docker-node-app:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
